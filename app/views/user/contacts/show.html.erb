<div class="container">
  <div class="row mt-5 mb-5">
    <div class="col-md-5">
      <div class="card rounded-0">
        <div class="card-body">
          <h5 class="text-muted font-weight-bold">Name</h5>
          <h6 class="text-muted font-14"><%= @contact.name %></h6>
          <hr>
          <h5 class="text-muted font-weight-bold">Email</h5>
          <h6 class="text-muted font-14"><%= @contact.email %></h6>
          <hr>
          <h5 class="text-muted font-weight-bold">Contact Number</h5>
          <h6 class="text-muted font-14"><%= @contact.contact_number %></h6>
          <hr>
          <h5 class="text-muted font-weight-bold">Message</h5>
          <p class="text-muted font-14"><%= @contact.message %></p>
        </div>
      </div>
    </div>
    <div class="col-md-7">
      <div class="card rounded-0">
        <div class="card-body">
        </div>
      </div>
    </div>
  </div>
</div>
